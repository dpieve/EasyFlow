<UserControl
    x:Class="EasyFlow.Presentation.Features.Settings.SettingsView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:general="clr-namespace:EasyFlow.Presentation.Features.Settings.General"
    xmlns:icons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
    xmlns:local="clr-namespace:EasyFlow.Presentation.Features.Settings"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:suki="clr-namespace:SukiUI.Controls;assembly=SukiUI"
    xmlns:tags="clr-namespace:EasyFlow.Presentation.Features.Settings.Tags"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="local:SettingsViewModel"
    mc:Ignorable="d">

    <ScrollViewer>
        <WrapPanel
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Orientation="Horizontal">
            <tags:TagsView
                Width="400"
                Height="370"
                Margin="5,10,25,5"
                DataContext="{Binding Tags}" />
            <!--<general:GeneralSettingsView
                Width="400"
                Height="370"
                Margin="5,10,5,5"
                DataContext="{Binding GeneralSettings}" />-->
        </WrapPanel>
    </ScrollViewer>

</UserControl>